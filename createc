    state: boolean
}" >> types.ts

echo "import scss from './$1.module.scss';
import { I$1Props } from './types';

export const $1 = (props: I$1Props) => {
    return (
        <div className={scss.container}>
        </div>
    )
}
    " >> $1.tsx

